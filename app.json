{
  "expo": {
    "name": "KitchAI",
    "slug": "kitchai-v2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "description": "AI-powered recipe discovery and meal planning app. Scan your pantry, get personalized recipes, and plan your meals with intelligent recommendations.",
    "keywords": [
      "recipe",
      "cooking",
      "AI",
      "meal planning",
      "food",
      "pantry",
      "kitchen",
      "nutrition"
    ],
    "primaryColor": "#FF6B35",
    "scheme": "kitchai",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chieftitan.kitchai-v2",
      "buildNumber": "1",
      "associatedDomains": [
        "applinks:kitchai.app",
        "applinks:*.kitchai.app"
      ],
      "infoPlist": {
        "NSCameraUsageDescription": "KitchAI uses your camera to scan pantry items and identify ingredients for personalized recipe recommendations.",
        "NSPhotoLibraryUsageDescription": "KitchAI accesses your photo library to let you select images of ingredients for recipe suggestions.",
        "CFBundleDisplayName": "KitchAI",
        "CFBundleName": "KitchAI",
        "CFBundleURLTypes": [
          {
            "CFBundleURLName": "com.chieftitan.kitchai-v2",
            "CFBundleURLSchemes": ["kitchai"]
          }
        ]
      },
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "package": "com.chieftitan.kitchaiv2",
      "versionCode": 1,
      "compileSdkVersion": 34,
      "targetSdkVersion": 34,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true,
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "kitchai.app"
            },
            {
              "scheme": "https", 
              "host": "*.kitchai.app"
            },
            {
              "scheme": "kitchai"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "extra": {
      "eas": {
        "projectId": "06cbb30c-9d18-474e-afbb-b93bed7e5b42"
      }
    },
    "plugins": [
      "expo-font",
      "expo-video"
    ],
    "privacy": "public"
  }
}
