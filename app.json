{
  "expo": {
    "name": "KitchAI v2",
    "slug": "kitchai-v2",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "assets/**/*",
      "!**/*PRD*.md",
      "!**/*CONFIDENTIAL*.md",
      "!**/*INTERNAL*.md",
      "!**/*STRATEGY*.md",
      "!docs/prd-archive/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chieftitan.kitchaiv2",
      "buildNumber": "1"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.chieftitan.kitchaiv2",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow KitchAI to access your camera to scan pantry items."
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow KitchAI to access your photos to save and select recipe images.",
          "savePhotosPermission": "Allow KitchAI to save photos to your device.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "scheme": "kitchai-v2",
    "experiments": {
      "typedRoutes": true
    }
  }
}
